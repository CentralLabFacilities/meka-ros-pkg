  <macro name="m3_l_hand" params="side parent reflect use_mimic:=false *origin">
    <m3_l_palm side="${side}" reflect="${reflect}" parent="${parent}" use_mimic="${use_mimic}">
      <insert_block name="origin" />
    </m3_l_palm>
  </macro>


